div(class='panel panel-default')
    div(class='panel-heading') Excersises    
    div(class='panel-body')
        table(data-bind='foreach: excerciseList', style='width: 100%')
            tr(class='selectable-row', data-bind='click: $parent.onSelect')
                td
                    p(data-bind='text: $data.title')
    div(class='panel-footer') 
        input(type='button', class='btn btn-primary', value='New Excercise', data-bind='click: $parent.addNewElement')